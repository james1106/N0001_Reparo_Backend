contract HelloWorld{
    uint balance;
    function update(uint amount) returns (address, uint){
        balance += amount;
        return (msg.sender, balance);
    }

    //uint8表示八位无符号数
    function testUint8(uint8 amoun) returns (uint){
        uint8 v_unit8 = amoun;
        return v_unit8;
    }

    function testBytes1(bytes1 amoun) returns (bytes1){
        bytes1 v_bytes8 = amoun;
        return v_bytes8;
    }

}

contract HelloWorld222{
    uint balance;
    function update(uint amount) returns (address, uint){
        balance += amount;
        return (msg.sender, balance);
    }

    //uint8表示八位无符号数
    function testUint8(uint8 amoun) returns (uint){
        uint8 v_unit8 = amoun;
        return v_unit8;
    }

    function testBytes1(bytes1 amoun) returns (bytes1){
        bytes1 v_bytes8 = amoun;
        return v_bytes8;
    }

}